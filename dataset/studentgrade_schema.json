{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": [
    {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "title": "Studentgrade Object",
      "description": "studentgrade data input",
      "properties": {
        "course": {
          "type": "string",
          "pattern": "[EE|CS][0-9]{2}"
        },
        "grade": {
          "type": "integer",
          "minimum": 0,
          "maximum": 100
        }
      }
    }
  ]
}